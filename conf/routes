GET     /apparels                   controllers.WardrobeController.searchApparels()
POST    /uploadWardrobe             controllers.WardrobeController.upload()
GET     /wardrobe                   controllers.WardrobeController.wardrobe()
GET     /tagOutfit                  controllers.WardrobeController.tagOutfit()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
